{
    "parser": "babel-eslint",
    "plugins": [
      "react",
      "react-native"
    ],
    "parserOptions": {
      "ecmaFeatures": {
          "jsx": true,
          "modules": true
      }
    },
    "extends" : [
      "eslint:recommended",
      "plugin:react/recommended",
      "airbnb"
    ],
    "rules" : {
      "arrow-body-style": "warn",
      "react/jsx-filename-extension": "off",
      "react/prop-types": "off"
    }
  }